{
  "name": "@qc2168/mib",
  "version": "1.3.3",
  "description": "data backup tool",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/es/index.mjs",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "lint": "eslint ./src/**/*.ts --fix && tsc",
    "dev": "node scripts/dev.js",
    "types": "tsc --emitDeclarationOnly --outDir dist",
    "prebuild": "rmdir /s/q dist",
    "build": "pnpm prebuild && pnpm lint && node scripts/build.js && pnpm types",
    "commit": "gitmoji -c",
    "release": "pnpm lint && pnpm build && release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QC2168/mib-cli.git"
  },
  "keywords": [
    "adb",
    "mib",
    "mib-cli",
    "backup"
  ],
  "author": "QC2168",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "bin": {
    "mib": "dist/bin/index.cjs"
  },
  "bugs": {
    "url": "https://github.com/QC2168/mib-cli/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "homepage": "https://github.com/QC2168/mib-cli#readme",
  "devDependencies": {
    "@types/core-js": "^2.5.5",
    "@types/node": "^17.0.10",
    "@types/prompts": "^2.0.14",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "gitmoji-cli": "^8.0.0",
    "release-it": "^15.8.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "esbuild": "^0.17.15",
    "pnpm": "^7.0.0",
    "prompts": "^2.4.2",
    "regenerator-runtime": "^0.13.9",
    "winston": "^3.5.1"
  }
}
